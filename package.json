{
  "name": "capitalise-post-town",
  "version": "2.0.2",
  "description": "Capitalise UK post towns correctly",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm run lint && NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json lib/**/*.ts",
    "build": "npm run lint && node_modules/.bin/tsc",
    "coverage": "node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls || true",
    "prepare": "npm run build"
  },
  "mocha": {
    "spec": "test/*.ts",
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ideal-postcodes/capitalise-post-town.git"
  },
  "keywords": [
    "capitalise",
    "Post",
    "Town",
    "UK",
    "Addressing"
  ],
  "author": "Ideal Postcodes",
  "license": "MIT",
  "runkitExampleFilename": "example.js",
  "bugs": {
    "url": "https://github.com/ideal-postcodes/capitalise-post-town/issues"
  },
  "homepage": "https://github.com/ideal-postcodes/capitalise-post-town#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "~4.1.3",
    "typescript": "~3.3.1",
    "@types/mocha": "~5.2.0",
    "@types/node": "~11.11.0",
    "chai": "~4.2.0",
    "coveralls": "^3.0.1",
    "mocha": "~6.0.0",
    "nyc": "~13.3.0",
    "source-map-support": "~0.5.6",
    "ts-node": "~8.0.2",
    "tslint": "~5.14.0"
  }
}
